{
  "name": "eng-react",
  "version": "1.0.0",
  "description": "A personal react component library",
  "author": "eng618",
  "license": "MIT",
  "repository": "eng618/eng-react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write \"src/**/*.{scss,css,js,md,mdx}\"",
    "format:diff": "prettier --check \"src/**/*.{scss,css,js,md,mdx}\"",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn run build",
    "start": "rollup -c -w",
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@eng618/prettier-config": "^1.6.0",
    "@svgr/rollup": "^5.5.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.2.0",
    "gh-pages": "^3.1.0",
    "postcss": "^8.2.12",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^2.45.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-url": "^3.0.1",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist"
  ],
  "prettier": "@eng618/prettier-config"
}
